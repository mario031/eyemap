      src="https://maps.googleapis.com/maps/api/js?">
    </script>

    <style type="text/css">
    	* {
  			margin: 0 auto;
		}
		html,
		body {
  			height: 100%;
		}
        #map{
        	width: 100%;
  			height: 100%;
        }
        </style>
    <script type="text/javascript">
    		
    		var lat1 = doRad(35.388675062);
  			var lng1 = doRad(139.4263787103);
  			var alpha1 = doRad(15.46);
  			var length1 = 30;
  
  			var an1 = new Array();
  			an1 = vincenty(lat1,lng1,alpha1,length1); 
  			var lat2 = doRad(an1[0]);
  			var lng2 = doRad(an1[1]);
  			var alpha2 = doRad(105.46);
  			var alpha3 = doRad(285.46);
  			var length2 = 5;
  			var an2 = new Array();
  			an2 = vincenty(lat2,lng2,alpha2,length2); 
  			var an3 = new Array();
  			an3 = vincenty(lat2,lng2,alpha3,length2); 
  			//var latlng2 = String(an2[0]) + '\t' + String(an2[1]);
  			

            function init() {
                var latlng = new google.maps.LatLng(35.388675062, 139.4263787103);
                var myoptions = {
                    zoom: 20,
                    center: latlng,
                    mapTypeId: google.maps.MapTypeId.ROAD,
                    streetViewControl: true,
                };
                var map = new google.maps.Map(document.getElementById("map"), myoptions);

                var triangleCoords = [
    				{lat: 35.388675062, lng: 139.4263787103},
    				{lat: an2[0], lng: an2[1]},
    				{lat: an3[0], lng: an3[1]}
  				];

  				// Construct the polygon.
 				var bermudaTriangle = new google.maps.Polygon({
    				paths: triangleCoords,
    				strokeColor: '#FF0000',
    				strokeOpacity: 0.8,
    				strokeWeight: 0,
    				fillColor: '#0000ff',
    				fillOpacity: 0.7
  				});
  				bermudaTriangle.setMap(map);
			}
    </script>
	<title>Eye Map</title>
</head>
<body onload="init();">
	<div id="map""></div>
</body>
</html>